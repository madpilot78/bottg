{
    "name": "madpilot78/bottg",
    "description": "basic Telegram bot class",
    "keywords": [
        "telegram",
        "bot",
        "php"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Guido Falsi",
            "email": "mad@madpilot.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.4",
        "php-mock/php-mock-phpunit": "^2.1",
        "symfony/yaml": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "madpilot78\\bottg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "madpilot78\\bottg\\tests\\": "tests/"
        }
    },
    "scripts": {
        "sniff": "phpcs",
        "test": "phpunit",
        "cover": "phpunit --coverage-html tests/coverage"
    }
}
